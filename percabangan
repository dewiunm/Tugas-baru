#include <iostream>
using namespace std;
int main () {
int x;
int y;
int z;
cout << "Masukan bilangan x:" ;
cin >> x;
cout << "Masukan bilangan y:" ;
cin >> y;
z=x+y;
cout <<x<<"+"<<y<<""<<"="<<z<<endl;
if (z>=0) {
}
if (z>=0) {
cout <<" Nilai "<<z<<" positif "<<endl;
}else if (z<0) {
cout <<" Nilai "<<z<<" negatif "<<endl;
}
cout<<"Program ini adalah progrm berhitung"<<endl;
return 0;
}
